	<UseWindowsForms>true</UseWindowsForms>
	<Platforms>AnyCPU;x64;x86</Platforms>
	<Configurations>Debug;Release</Configurations>
	<ApplicationIcon>Icon.ico</ApplicationIcon>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="Setting\**" />
    <Compile Remove="SYMBOL\**" />
